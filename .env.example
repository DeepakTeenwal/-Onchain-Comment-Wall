# Private key for contract deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# RPC URLs for blockchain networks
CELO_RPC_URL=https://forno.celo.org
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# Block explorer API keys for contract verification
CELOSCAN_API_KEY=your_celoscan_api_key
ARBISCAN_API_KEY=your_arbiscan_api_key

# Contract addresses (fill after deployment)
NEXT_PUBLIC_CELO_CONTRACT_ADDRESS=
NEXT_PUBLIC_ARBITRUM_CONTRACT_ADDRESS=

# Farcaster / Neynar API (get from https://neynar.com)
NEXT_PUBLIC_NEYNAR_CLIENT_ID=your_neynar_client_id

# WalletConnect Project ID (get from https://cloud.walletconnect.com)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3000
